{
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.3.1",
    "helmet": "^7.1.0",
    "http-proxy-middleware": "^3.0.3",
    "jsonwebtoken": "^9.0.2",
    "pino": "^9.5.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@swc/core": "^1.7.18",
    "@swc/jest": "^0.2.36",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20.14.10",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.7",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.7",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^11.3.0",
    "sucrase": "^3.35.0",
    "supertest": "^7.0.0",
    "swagger-ui-express": "^5.0.1",
    "typescript": "^5.6.3",
    "uglify-js": "^3.19.3"
  },
  "name": "api-gateway",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "scripts": "npm pkg get scripts",
    "test": "jest",
    "test:silent": "jest --silent",
    "test:watch": "jest --watchAll",
    "test:e2e": "jest --bail --projects ./tests/e2e",
    "test:e2e:silent": "jest --bail --silent --projects ./tests/e2e",
    "test:e2e:watch": "jest --bail  --watchAll --projects ./tests/e2e",
    "start:dev": "nodemon ./src/index.ts",
    "start:prod": "node build/src/index.js",
    "api-docs": "nodemon docs/api/openApi/apiDocs.ts",
    "lint-staged": "lint-staged",
    "lint:check": "biome lint .",
    "format:fix": "biome format --write .",
    "fix": "biome check --write .",
    "build:dev": "tsc",
    "build:dev:incremental": "tsc --incremental -p tsconfig.json",
    "build": "tsc -p tsconfig.json && node buildUtils/minify.js",
    "revise": "npm-run-all --parallel test:silent test:e2e:silent build:dev fix",
    "revise:pre-commit": "npm-run-all --parallel test:silent test:e2e:silent build:dev:incremental lint-staged",
		"lint:dockerfile:windows": "type dockerfile | docker run --rm -i hadolint/hadolint",
		"lint:dockerfile:linux": "cat dockerfile | docker run --rm -i hadolint/hadolint",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "Lucas Felipe Timoteo",
  "license": "MIT",
  "type": "commonjs",
  "description": "Api gateway"
}